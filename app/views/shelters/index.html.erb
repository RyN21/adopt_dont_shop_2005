<p><%= link_to "Shelter Index", "/shelters" %></p>
<p><%= link_to "Pet Index", "/pets" %></p>

<% @shelters.each do |shelter| %>
  <h2><%= link_to "#{shelter.name}", "/shelters/#{shelter.id}" %></h2>
  <p><%=  link_to "Edit #{shelter.name}", "/shelters/#{shelter.id}/edit" %></p>
  <p><%= button_to "Delete #{shelter.name}", "/shelters/#{shelter.id}", method: :delete %></p>
<% end %>

<%= link_to "New Shelter", "/shelters/new" %>
